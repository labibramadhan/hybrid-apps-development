{
  "name": "cbt-exp1",
  "productName": "Computer Based Test",
  "version": "1.0.0",
  "description": "CBT Experiment 1 with react + webpack + babel + electron",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-desktop": "./node_modules/.bin/electron-packager ./ --platform=darwin --arch=x64 --out=releases --ignore=platforms --ignore=plugins --ignore=releases --prune=true --overwrite=true",
    "build-android": "./node_modules/.bin/cordova build android",
    "desktop": "./node_modules/.bin/electron .",
    "android": "./node_modules/.bin/cordova run android",
    "dev": "./node_modules/.bin/npm-run-all --parallel gulp babel-electron",
    "gulp": "./node_modules/.bin/gulp default",
    "babel-electron": "babel Electron.jsx --watch --presets es2015 --out-file main.js"
  },
  "keywords": [
    "cbt",
    "experiment"
  ],
  "author": "Muhammad Labib Ramadhan",
  "license": "ISC",
